{
  "name": "medical-3d-imaging-tool",
  "version": "1.0.0",
  "description": "Simple medical 3D imaging tool with Webpack-dev-server and Cornerstone3D",
  "main": "src/index.js",
  "scripts": {
    "dev": "webpack serve --mode development",
    "dev-no-ws": "webpack --mode development --watch",
    "build": "webpack --mode production",
    "start": "npm run dev",
    "serve": "npm run build && npx http-server dist -p 3000 -o"
  },
  "keywords": [
    "medical",
    "imaging",
    "3d",
    "dicom",
    "cornerstone3d",
    "webpack"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "html-webpack-plugin": "^5.5.3",
    "css-loader": "^6.8.1",
    "style-loader": "^3.3.3"
  },
  "dependencies": {
    "@cornerstonejs/core": "^3.31.6",
    "@cornerstonejs/tools": "^3.31.6",
    "@cornerstonejs/dicom-image-loader": "^3.31.6",
    "dicom-parser": "^1.8.21"
  }
}